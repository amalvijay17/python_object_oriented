class A:
    def feature1(self):
        print("Feature 1 working")

    def feature2(self):
        print("Feature 2 working")


class B(A):  # Single Level Inheritance
    def feature3(self):
        print("Feature 3 working")

    def feature4(self):
        print("Feature 4 working")


class C(B):  # Multilevel Inheritance
    def feature5(self):
        print("Feature 5 working")


a1 = A()
b1 = B()
c1 = C()
#  Super Class or Parent Class, Sub Class or Child class
#  Class which inherits the properties are called Sub Class or Child Class
a1.feature2()
b1.feature4()
c1.feature5()
