#  Method Overloading
class Student:
    def sum(self, a=None, b=None, c=None):
        if a != None and b != None and c != None:
            s = a + b + c
        elif a != None and b != None:
            s = a + b
        else:
            s = a
        return s


s1 = Student()  # s1 is an object in Student Class
print(s1.sum(10, 7))
print(s1.sum(9, 8, 6))
